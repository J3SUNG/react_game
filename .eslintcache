\n  const onRightClickTd = useCallback(\r\n    (e) => {\r\n      e.preventDefault();\r\n      if (halted) {\r\n        return;\r\n      }\r\n      switch (tableData[rowIndex][cellIndex]) {\r\n        case CODE.NORMAL:\r\n        case CODE.MINE:\r\n          dispatch({ type: FLAG_CELL, row: rowIndex, cell: cellIndex });\r\n          return;\r\n        case CODE.FLAG_MINE:\r\n        case CODE.FLAG:\r\n          dispatch({ type: QUESTION_CELL, row: rowIndex, cell: cellIndex });\r\n          return;\r\n        case CODE.QUESTION_MINE:\r\n        case CODE.QUESTION:\r\n          dispatch({ type: NORMALIZE_CELL, row: rowIndex, cell: cellIndex });\r\n          return;\r\n        default:\r\n          return;\r\n      }\r\n    },\r\n    [tableData[rowIndex][cellIndex], halted]\r\n  );\r\n\r\n  console.log(\"td rendered\");\r\n\r\n  return (\r\n    <RealTd\r\n      onClickTd={onClickTd}\r\n      onRightClickTd={onRightClickTd}\r\n      data={tableData[rowIndex][cellIndex]}\r\n    />\r\n  );\r\n});\r\n\r\nconst RealTd = memo(({ onClickTd, onRightClickTd, data }) => {\r\n  console.log(\"real td rendered\");\r\n  return (\r\n    <td\r\n      style={getTdStyle(data)}\r\n      onClick={onClickTd}\r\n      onContextMenu={onRightClickTd}\r\n    >\r\n      {getTdText(data)}\r\n    </td>\r\n  );\r\n});\r\n\r\nexport default Td;\r\n",{"ruleId":"107","replacedBy":"108"},{"ruleId":"109","replacedBy":"110"},{"ruleId":"111","severity":1,"message":"112","line":123,"column":6,"nodeType":"113","endLine":123,"endColumn":18,"suggestions":"114"},{"ruleId":"111","severity":1,"message":"115","line":35,"column":6,"nodeType":"113","endLine":35,"endColumn":16,"suggestions":"116"},{"ruleId":"117","severity":1,"message":"118","line":34,"column":3,"nodeType":"119","messageId":"120","endLine":34,"endColumn":8},{"ruleId":"121","severity":1,"message":"122","line":256,"column":13,"nodeType":"123","messageId":"124","endLine":256,"endColumn":18},{"ruleId":"121","severity":1,"message":"125","line":2,"column"